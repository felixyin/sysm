                        td
                            input.col-xs-10.col-sm-5(type='text' , name='report_handover', placeholder='请输入分析报告组接收人')
                        td 分析报告组接收时间
                        td
                            input.col-xs-10.col-sm-5.ipt-date(type='text' , name='report_handover_date', placeholder='请输入分析报告组接收时间')
