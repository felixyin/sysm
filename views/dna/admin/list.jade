                            select#status.col-xs-12.clo-sm-10(style="width:180px;", name='status',placeholder='')
                                option(value='-1') 全部
                                option(value='0') 已废弃
                                option(value='1') 已录入
                                option(value='2') 已审批
                                option(value='3') 已入库
                                option(value='4') 已出库
                                option(value='5') 已提取
                                option(value='6') 提取合格
                                option(value='7') 提取废弃
                                option(value='8') 重提取
                                option(value='9') 提取已交接
                                option(value='10') 已建库
                                option(value='11') 建库合格
                                option(value='12') 建库废弃
                                option(value='13') 重建库
                                option(value='14') 建库已交接
                                option(value='15') 已上机
                                option(value='16') 上机合格
                                option(value='17') 上机废弃
                                option(value='18') 重上机
                                option(value='19') 上机已交接
                                option(value='20') 已分析
                                option(value='21') 报告已发送
                        td 显示历史数据
                        td
                            select.col-xs-12.clo-sm-10(style="width:180px;", name='show_is_his', placeholder='')
                                option(value='') 全部数据
                                option(value='1', selected) 正向流程数据
                                option(value='2') 逆向流程数据
