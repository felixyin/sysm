extends ../../layout_nomenu


block content

    style.
        .my-toolbar > .btn-group, .my-toolbar > button {
            margin-right: 10px;
        }

        .btn-group > .btn-sm, .btn-group + .btn-sm {
            border-width: 4px;
        }

    .main-content
        .page-content
            form#search-form-id
                table.searchTable
                    tr
                        td 条码编号
                        td
                            input.col-xs-10.col-sm-5(type='text' , name='barcode_long', placeholder='请输入条码编号')
                        td qbite浓度
                        td
                            input.col-xs-10.col-sm-5(type='text' , name='extract_qbite_deep', placeholder='请输入qbite浓度')
                        td epoch浓度
                        td
                            input.col-xs-10.col-sm-5(type='text' , name='extract_epoch_deep', placeholder='请输入epoch浓度')
                    tr
                        td 纯度
                        td
                            input.col-xs-10.col-sm-5(type='text' , name='extract_purity_deep', placeholder='请输入纯度')
                        td 片段大小
                        td
                            input.col-xs-10.col-sm-5(type='text' , name='extract_part_size', placeholder='请输入片段大小')
                        td 打断后片段
                        td
                            input.col-xs-10.col-sm-5(type='text' , name='extract_part_after_break', placeholder='请输入打断后片段')
                    tr
                        td 提取人员
                        td
                            input.col-xs-10.col-sm-5.ipt-person(type='text' , name='extracter', placeholder='请选择提取人员')
                        td 提取时间
                        td
                            input.col-xs-10.col-sm-5.ipt-date(type='text' , name='extract_date', placeholder='请选择提取时间')
                        td 提取审核人
                        td
                            input.col-xs-10.col-sm-5.ipt-person(type='text' , name='extract_checker', placeholder='请选择提取审核人')
                    tr
                        td 提取审核时间
                        td
                            input.col-xs-10.col-sm-5.ipt-date(type='text' , name='extract_check_date', placeholder='请选择提取审核时间')
                        td 提取出库人
                        td
                            input.col-xs-10.col-sm-5.ipt-person(type='text' , name='extract_outer', placeholder='请选择提取出库人')
                        td 提取组试管剩余数量
                        td
                            input.col-xs-10.col-sm-5(type='text' , name='extract_out_residue', placeholder='请输入提取组试管剩余数量')
                    tr
                        td 状态
                        td
                            select#status.col-xs-12.clo-sm-10(style="width:180px;", name='status',placeholder='')
                                option(value='-1') 全部
                                option(value='4') 交接后未提取
                                option(value='5') 提取且已保存
                                option(value='6') 提取审核-合格
                                option(value='7') 提取审核-废弃
                                option(value='8') 提取审核-重提取
                                option(value='9') 交接后未建库
                        td
                        td
                        td
                        td
                            #btn-search.btn-group
                                button.btn.btn-info.btn-sm(type='submit')
                                    i.icon-edit.bigger-110 &nbsp;&nbsp;&nbsp;搜索
                                button.btn.btn-info.dropdown-toggle.btn-sm(data-toggle='dropdown')
                                    i.icon-angle-down.icon-only
                                ul.dropdown-menu.dropdown-yellow
                                    li
                                        a#btn-search-more(href='javascript:void(0);') 显示更多搜索项
                            button.btn.btn-sm(type='reset')
                                i.icon-undo.bigger-110 重置

            div.my-toolbar
                #btn-cxd.btn-group
                    button.btn.btn-info.btn-sm(onclick="showEditDialog(this)")
                        i.icon-edit.bigger-110 &nbsp;&nbsp;&nbsp;录入DNA提取信息
                    button.btn.btn-info.dropdown-toggle.btn-sm(data-toggle='dropdown')
                        i.icon-angle-down.icon-only
                    ul.dropdown-menu.dropdown-yellow
                        li
                            a(href='javascript:clearUser("DNA提取录入人员")') 不记住DNA提取录入人员
                        //li
                            a(href='#') Separated link
                span
                #btn-sh.btn-group
                    button.btn.btn-info.btn-sm(onclick="showShDialog(this)",disabled)
                        i.icon-edit.bigger-110 &nbsp;&nbsp;&nbsp;审核
                    button.btn.btn-info.dropdown-toggle.btn-sm(data-toggle='dropdown',disabled)
                        i.icon-angle-down.icon-only
                    ul.dropdown-menu.dropdown-yellow
                        li
                            a(href='javascript:clearUser("审核人员")') 不记住审核人员
                span
                //button#btn-rk-id.btn.btn-info.btn-sm(onclick="rk(this)")
                    i.icon-inbox.bigger-110 入库
                #btn-ck.btn-group
                    button.btn.btn-info.btn-sm(onclick="showCkDialog(this)")
                        i.icon-edit.bigger-110 &nbsp;&nbsp;&nbsp;批量出库
                    button.btn.btn-info.dropdown-toggle.btn-sm(data-toggle='dropdown')
                        i.icon-angle-down.icon-only
                    ul.dropdown-menu.dropdown-yellow
                        li
                            a(href='javascript:clearUser("出库人员")') 不记住出库人员
                        li
                            a(href='javascript:clearUser("交接人员")') 不记住交接人员
                span
                button#btn-dc-id.btn.btn-default.btn-sm(onclick="exportCvs(this)")
                    i.icon-download-alt.bigger-110 导出EXCEL
                //button#btn-dy-id.btn.btn-default.btn-sm(onclick="dy(this)")
                    i.icon-print.bigger-110 打印

            table#grid-table
            #grid-pager
    script(type='text/javascript',src="javascripts/dna/extract.js")
